.iner_title_sub {float:left; width:100%; line-height:normal;}
.iner_title_sub ul {float:left; padding:0px; margin:0px; font-size:14px; overflow:hidden; list-style-type:none;}
.iner_title_sub ul li {display:inline; margin:0; padding:0;}
.iner_title_sub ul li a {float:left; background:url("../images/sub_bg_left.png") no-repeat left -27px; margin:0; padding:0 2px 0 6px; text-decoration:none;}
.iner_title_sub ul li a span {float:left; color:#062b57; cursor:pointer; font-weight:bold; display:block; background:url("../images/sub_bg_right.png") no-repeat right -27px; padding:5px 15px 4px 6px; color:#1e5494;}
/* Commented Backslash Hack hides rule from IE5-Mac \*/
.iner_title_sub a span {float:none;}
/* End IE5-Mac hack */
.iner_title_sub a:hover span {color:#062b57; font-weight:bold; cursor:pointer;}
.iner_title_sub a:hover {background-position:0% -27px; color:#062b57; font-weight:bold;}
.iner_title_sub a:hover span {background-position:100% -27px; font-weight:bold;} 

.iner_title_sub ul li a.trigger_link {float:left; background:url("../images/sub_bg_left.png") no-repeat left top; margin:0; padding:0 2px 0 6px; text-decoration:none;}
.iner_title_sub ul li a.trigger_link span {float:left; color:#062b57; cursor:pointer; font-weight:bold; display:block; background:url("../images/sub_bg_right.png") no-repeat right top; padding:5px 15px 4px 6px;}
/* Commented Backslash Hack hides rule from IE5-Mac \*/
.iner_title_sub a.trigger_link span {float:none;}
/* End IE5-Mac hack */
.iner_title_sub a:hover span {color:#062b57; font-weight:bold; cursor:pointer;}
.iner_title_sub a:hover {background-position:0% 0px; color:#062b57; font-weight:bold;}
.iner_title_sub a:hover span {background-position:100% 0px; font-weight:bold;}  




