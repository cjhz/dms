package com.sunraysoft.hr.domain.dto.report;

/**
 * 机构统计dto
 * @author juyf
 * @date 2012-08-16
 */
public class DeptStatisticsDto {
	private Long areaid;// 地区id
	private String areacode;// 地区编码
	private Long departmenttype;// 机关类型
	private Long deptype;// 机构性质
	private Long deptdict;// 机构职级
	private Long deptobserving;// 机构立功表彰

	public Long getAreaid() {
		return areaid;
	}

	public void setAreaid(Long areaid) {
		this.areaid = areaid;
	}

	public String getAreacode() {
		return areacode;
	}

	public void setAreacode(String areacode) {
		this.areacode = areacode;
	}

	public Long getDepartmenttype() {
		return departmenttype;
	}

	public void setDepartmenttype(Long departmenttype) {
		this.departmenttype = departmenttype;
	}

	public Long getDeptype() {
		return deptype;
	}

	public void setDeptype(Long deptype) {
		this.deptype = deptype;
	}

	public Long getDeptdict() {
		return deptdict;
	}

	public void setDeptdict(Long deptdict) {
		this.deptdict = deptdict;
	}

	public Long getDeptobserving() {
		return deptobserving;
	}

	public void setDeptobserving(Long deptobserving) {
		this.deptobserving = deptobserving;
	}
}
